<% @servers.each do |server| -%>
-
  url: <%= server[:url] %>
  <%= (server[:username]) ? "username: #{server[:username]}" : "" %>
  <%= (server[:password]) ? "password: #{server[:password]}" : "" %>
  <% if server[:ignore_jobs] %>
  ignore_jobs:
    <% server[:ignore_jobs].each do |ignore| %>
    - <%= ignore %>
    <% end %>
  <% end %>
  <% if server[:jobs] -%>
  jobs:
    <% server[:jobs].each do |job| %>
    - <%= job %>
    <% end %>
  <% end %>
<% end %>
